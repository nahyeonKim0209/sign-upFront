{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "workbench.iconTheme": "material-icon-theme",
  "eslint.alwaysShowStatus": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Community Material Theme Ocean High Contrast",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "git.ignoreMissingGitWarning": true,
  "[typescript]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "files.associations": {
    "*.html": "html",
    "*.tsx": "typescriptreact"
  },
  "workbench.settings.useSplitJSON": true,
  "files.autoSave": "afterDelay",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": true
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "window.zoomLevel": 0,
  "editor.definitionLinkOpensInPeek": true,
  "editor.gotoLocation.alternativeImplementationCommand": "editor.action.goToReferences",
  "editor.gotoLocation.alternativeReferenceCommand": "editor.action.goToReferences",
  "editor.gotoLocation.multipleDefinitions": "goto"
}
